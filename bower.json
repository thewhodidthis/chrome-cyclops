{
  "name": "cyclops-chrome",
  "version": "0.0.1",
  "authors": [
    "Sotiris Bakagiannis <sotirios.bakagiannis@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/thewhodidthis/cyclops-chrome",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "src/js/vendor/",
    "test",
    "tests"
  ],
  "dependencies": {
    "AppResourceLoader": "latest",
    "socket.io-client": "latest"
  },
  "install": {
